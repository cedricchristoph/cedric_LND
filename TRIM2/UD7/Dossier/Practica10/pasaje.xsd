<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.misitio.com" elementFormDefault="qualified">
    
    <xs:element name="pasajes">
        <xs:complexType>
            <xs:sequence>
                
                <xs:element name="pasaje">
                    <xs:complexType>
                        <xs:attribute name="asiento" type="xs:string" use="required"/>
                        <xs:attribute name="ventanilla" type="xs:boolean" default="true"/>
                        <xs:attribute name="precio" tyoe="xs:double" use="optional"/>
                        <xs:attribute name="moneda" type="xs:string" default="euros"/>
                    </xs:complexType>
                </xs:element>
                
                
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
